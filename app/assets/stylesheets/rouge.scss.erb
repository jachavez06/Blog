<%= Rouge::Themes::Base16.render(:scope => '.highlight') %>

.highlight {
  background-color: #f5f5f5;
  padding: 0;
  margin: 1rem;
}
.codebox{
  border: 1px solid #c4c4c4;
  background-color: #f5f5f5;
  white-space: pre-line;
  padding: 8px 0px;
  font-size: 0.9em;
  width: 100%;
  border-radius: 5px;
}