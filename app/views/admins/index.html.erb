<div class="card">
    <div class="card-header">
        <ul class="nav nav-tabs card-header-tabs">
            <li class="nav-item">
                <a class="nav-link active" href="#">Articles</a>
            </li>
        </ul>
    </div>
    <div class="card-block">
        <h4 class="card-title">Unpublished</h4>
        <div class="panel panel-default">
            <div class="panel-body">
                <%= render 'articles/article_list', obj: @unpublished %>
                <%= will_paginate(@unpublished, :renderer => PaginateHelper::LinkRenderer) %>
            </div>
        </div>
        <%= link_to 'New', new_article_path(), class: "btn btn-success float-sm-right" %>
    </div>
</div>